{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "desktop-capability",
  "windows": ["main"],
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "permissions": [
    "updater:default",
    "core:window:allow-set-size",
    "notification:allow-is-permission-granted",
    "dialog:allow-open",
    "dialog:allow-save"
  ]
}
